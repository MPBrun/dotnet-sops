{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommitsDisabled"
  ],
  "ignorePresets": [
    ":ignoreModulesAndTests"
  ],
  "regexManagers": [
    {
      "fileMatch": ["SopsDownloadService.cs$"],
      "matchStrings": ["private const string Version = \"(?<currentValue>.*?)\";\\n"],
      "datasourceTemplate": "go",
      "depNameTemplate": "go.mozilla.org/sops/v3"
    }
  ]
}
